<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools" tools:ignore="ResourceName">
    <declare-styleable name="DashboardView">
        <attr name="radius" format="dimension"/>     <!--扇形半径-->
        <attr name="padding" format="dimension"/>     <!--上下间距-->
        <attr name="dashWidth" format="dimension"/>     <!--刻度宽度-->
        <attr name="dashHeight" format="dimension"/>     <!--刻度高度-->
        <attr name="topOffset" format="dimension"/>     <!--顶部偏移量-->
        <attr name="startAngle" format="integer"/> <!-- 起始角度-->
        <attr name="sweepAngle" format="integer"/>  <!--绘制角度-->
        <attr name="bigSliceCount" format="integer"/> <!-- 长刻度条数-->
        <attr name="sliceCountInOneBigSlice" format="integer"/> <!-- 长刻度条数-->
        <attr name="arcColor" format="color"/> <!--弧度颜色-->
        <attr name="measureTextSize" format="dimension"/> <!--刻度字体大小-->
        <attr name="innerTextColor" format="color"/> <!--字体颜色-->
        <attr name="headerTitle" format="string"/> <!--表头-->
        <attr name="headerTextSize" format="string"/> <!--表头字体大小-->
        <attr name="headerRadius" format="dimension"/> <!--表头半径-->
        <attr name="pointerRadius" format="dimension"/> <!--指针半径-->
        <attr name="circleRadius" format="dimension"/> <!--中心圆半径-->
        <attr name="minValue" format="integer"/> <!--最小值-->
        <attr name="maxValue" format="integer"/> <!--最大值-->
        <attr name="realTimeValue" format="float"/> <!--实时值-->
        <attr name="stripeWidth" format="dimension"/> <!--色带宽度-->
        <attr name="stripeMode"> <!--色条显示位置-->
            <enum name="normal" value="0"/>
            <enum name="inner" value="1"/>
            <enum name="outer" value="2"/>
        </attr>
        <attr name="bgColor" format="color"/> <!--背景颜色-->
    </declare-styleable>
    <declare-styleable name="HalfDashBoardChart">
        <attr name="halfRadius" format="dimension"/>     <!--扇形半径-->
        <attr name="halfPadding" format="dimension"/>     <!--上下间距-->
        <attr name="halfDashLength" format="dimension"/>     <!--刻度长度-->
        <attr name="halfMaxValue" format="float"/>     <!--最大值-->
        <attr name="halfMinValue" format="float"/>     <!--最小值-->
        <attr name="halfScale" format="integer"/>     <!--小数保留位数-->
        <attr name="halfShowText" format="boolean"/> <!--是否显示百分比和左下角文字-->
        <attr name="halfTopTextSize" format="dimension"/>
        <attr name="halfBottomTextSize" format="dimension"/>
        <attr name="halfTopTextColor" format="color"/>
        <attr name="halfBottomTextColor" format="color"/>
        <attr name="halfBoardStartColor" format="string"/>
        <attr name="halfBoardEndColor" format="string"/>
        <attr name="halfTopTextSuffix" format="string"/>
    </declare-styleable>
    <declare-styleable name="GradientRoundedRectangleChart">
        <attr name="gradientRoundedRectangleStartColor" format="color"/>
        <attr name="gradientRoundedRectangleAngle" format="float"/>
        <attr name="gradientRoundedRectangleDrawable" format="reference"/>
        <attr name="gradientRoundedRectangleEndColor" format="color"/>
        <attr name="gradientRoundedRectangleShadowColor" format="color"/>
        <attr name="gradientRoundedRectangleBottomText" format="string"/>
        <attr name="gradientRoundedRectangleTextSuffix" format="string"/>
        <attr name="gradientRoundedRectangleTextSuffixSize" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="CirclePercentChart">
        <attr name="circlePercentStartColor" format="color"/>
        <attr name="circlePercentEndColor" format="color"/>
        <attr name="circlePercentBottomText" format="string"/>
        <attr name="circlePercentBottomTextSize" format="dimension"/>
        <attr name="circlePercentBottomTextColor" format="color"/>
        <attr name="circlePercentTopTextSize" format="dimension"/>
        <attr name="circlePercentTopTextColor" format="color"/>
        <attr name="circlePercentStyle">
            <enum name="percent" value="0"/><!--百分比-->
            <enum name="text" value="1"/><!--纯文本-->
        </attr>
    </declare-styleable>
    <declare-styleable name="DashBoardRateChart">
        <attr name="dashboardRateStartColor" format="color"/>
        <attr name="dashboardRateEndColor" format="color"/>
        <attr name="dashboardRateBaseColor" format="color"/>
        <attr name="dashboardBottomText" format="string"/>
    </declare-styleable>
    <declare-styleable name="GradientLineChart">
        <attr name="gradientLineStartColor" format="color"/>
        <attr name="gradientLineEndColor" format="color"/>
        <attr name="gradientLineTextColor" format="color"/>
        <attr name="gradientLineTextSize" format="dimension"/>
        <attr name="gradientLineHeight" format="dimension"/>
        <attr name="gradientLineTextPadding" format="dimension"/>
        <attr name="gradientLineSuffix" format="string"/>
    </declare-styleable>
    <declare-styleable name="DoubleCirclePercentChart">
        <attr name="doubleCirclePercentStartColor" format="color"/>
        <attr name="doubleCirclePercentEndColor" format="color"/>
        <attr name="doubleCirclePercentStartColor2" format="color"/>
        <attr name="doubleCirclePercentEndColor2" format="color"/>
        <attr name="doubleCircleBackgroundColor" format="color"/>
        <attr name="doubleCircleTopTextColor" format="color"/>
        <attr name="doubleCircleTopTextSize" format="dimension"/>
        <attr name="doubleCircleBottomTextColor" format="color"/>
        <attr name="doubleCircleBottomTextSize" format="dimension"/>
        <attr name="doubleCircleIsGradient" format="boolean"/>
        <attr name="doubleCirclePercentBottomText" format="string"/>
    </declare-styleable>
    <declare-styleable name="DonutsChart">
        <attr name="donutsRadius" format="dimension"/><!--甜甜圈半径-->
        <attr name="donutsRadiusStrokeWidth" format="dimension"/><!--甜甜圈画笔宽度-->
        <attr name="donutsRadiusInnerCircle" format="dimension"/><!--甜甜圈内圆半径-->
        <attr name="donutsRadiusInnerCircleColor" format="color"/><!--甜甜圈内圆画笔颜色-->
        <attr name="donutsRadiusInnerCircleStrokeWidth" format="dimension"/><!--甜甜圈内圆画笔宽度-->
        <attr name="donutsRadiusInnerCircleStyle"><!--甜甜圈内圆样式-->
            <enum name="fill" value="0"/><!--填充-->
            <enum name="stroke" value="1"/><!--描边-->
        </attr>
        <attr name="donutsRadiusOuterCircle" format="dimension"/><!--甜甜圈外圆半径-->
        <attr name="donutsRadiusOuterCirclePoint" format="dimension"/><!--甜甜圈外圆点半径,donutsRadiusOuterCirclePoint=(donutsRadiusOuterCircle+donutsRadius)/2或者自定义-->
        <attr name="donutsRadiusOuterCirclePointColor" format="color"/><!--甜甜圈外圆小圆点颜色-->
        <attr name="donutsRadiusOuterCircleColor" format="color"/><!--甜甜圈外圆画笔颜色-->
        <attr name="donutsRadiusOuterCircleStrokeWidth" format="dimension"/><!--甜甜圈外圆画笔宽度-->
        <attr name="donutsRadiusOuterCircleStyle"><!--甜甜圈外圆样式-->
            <enum name="fill" value="0"/><!--填充-->
            <enum name="stroke" value="1"/><!--描边-->
        </attr>
        <attr name="donutsRadiusInnerCircleShowDivider" format="boolean"/>
        <attr name="donutsRadiusOuterCircleShowStroke" format="boolean"/>
        <attr name="donutsRadiusOuterCircleShowPoint" format="boolean"/>
        <attr name="donutsRadiusOuterCircleShowText" format="boolean"/>
        <attr name="donutsRadiusShowOuterLineText" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="PolyTextView">
        <attr name="polyTextColumns" format="integer"/>
        <attr name="polyTextColor" format="color"/>
        <attr name="polyTextSize" format="dimension"/>
        <attr name="polyTextPadding" format="dimension"/>
        <attr name="polyTextOutPadding" format="dimension"/>
        <attr name="polyTextDrawablePadding" format="dimension"/>
        <attr name="polyTextOrientation"><!--view文本绘制方向-->
            <enum name="horizontal" value="0"/><!--默认水平-->
            <enum name="vertical" value="1"/><!--垂直方向-->
        </attr>
        <attr name="polyTextBackgroundRadius" format="dimension"/>
        <attr name="polyTextShowBackground" format="boolean"/>
        <attr name="polyTextBackgroundColor" format="color"/>
    </declare-styleable>
    <declare-styleable name="RoundRectangleView">
        <attr name="roundRectangleRadius" format="dimension"/>
        <attr name="roundRectangleColor" format="color"/>
    </declare-styleable>
</resources>